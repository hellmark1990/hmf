<script>
    PROJECT_URLS = {
        'book_search_google': '{{ path('book_search_google') }}'
    }
</script>
<script type="text/javascript" src="https://maps.google.com/maps/api/js?key={{ parameters.google_api_key }}&libraries=places"></script>
<script src="//cdn.tinymce.com/4/tinymce.min.js"></script>
<script>tinymce.init({selector: 'textarea'});</script>

{% block javascripts %}
    {% javascripts '@jquery_and_ui' %}
