<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>{% block title %}Welcome!{% endblock %}</title>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    <!-- core CSS -->
    {% block stylesheets %}
        {% stylesheets
        output='fonts/fontawesome-webfont.woff'
        '%kernel.root_dir%/Resources/public/fonts/fontawesome-webfont.woff' %}
        {% endstylesheets %}
        {% stylesheets
        output='fonts/FontAwesome.otf'
        '%kernel.root_dir%/Resources/public/fonts/FontAwesome.otf' %}
        {% endstylesheets %}
        {% stylesheets
        output='fonts/fontawesome-webfont.svg'
        '%kernel.root_dir%/Resources/public/fonts/fontawesome-webfont.svg' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/fontawesome-webfont.ttf'
        '%kernel.root_dir%/Resources/public/fonts/fontawesome-webfont.ttf' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/fontawesome-webfont.woff'
        '%kernel.root_dir%/Resources/public/fonts/fontawesome-webfont.woff' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/glyphicons-halflings-regular.eot'
        '%kernel.root_dir%/Resources/public/fonts/glyphicons-halflings-regular.eot' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/glyphicons-halflings-regular.svg'
        '%kernel.root_dir%/Resources/public/fonts/glyphicons-halflings-regular.svg' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/glyphicons-halflings-regular.ttf'
        '%kernel.root_dir%/Resources/public/fonts/glyphicons-halflings-regular.ttf' %}
        {% endstylesheets %}

        {% stylesheets
        output='fonts/glyphicons-halflings-regular.woff'
        '%kernel.root_dir%/Resources/public/fonts/glyphicons-halflings-regular.woff' %}
        {% endstylesheets %}


        {% stylesheets
        '@main_css' %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
        {% endstylesheets %}

        {% stylesheets '@AppBundle/Resources/public/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

        {% stylesheets '@BookBundle/Resources/public/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

    {% endblock %}

    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
